{
  "name": "slim-select",
  "description": "Slim advanced select dropdown",
  "version": "1.27.1",
  "author": "Brian Voelker <brian@webiswhatido.com> (http://webiswhatido.com)",
  "homepage": "https://slimselectjs.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianvoe/slim-select/issues"
  },
  "main": "dist/slimselect.min.js",
  "module": "dist/slimselect.esm.js",
  "unpkg": "dist/slimselect.min.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "browser": {
    "./sfc": "src/slimselect.vue"
  },
  "style": "dist/slimselect.min.css",
  "sass": "src/slim-select/slimselect.scss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianvoe/slim-select.git"
  },
  "keywords": [
    "select",
    "vanilla",
    "dropdown",
    "search",
    "multiselect"
  ],
  "scripts": {
    "dev": "vite --port=8080",
    "library": "rm -r dist && cd src/slim-select && webpack && cd ../../ && npm run cleanDist && npm run renameDist && npm run mjs",
    "clean": "rimraf ./dist",
    "build": "npm run clean & npm run build:docs & npm run build:library",
    "build:docs": "vue-cli-service build",
    "build:library": "rollup --config src/slim-select/rollup.config.mjs"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/downloadjs": "^1.4.3",
    "@vitejs/plugin-vue": "^3.1.2",
    "clipboard": "^2.0.11",
    "downloadjs": "^1.4.7",
    "prismjs": "^1.29.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "sass": "^1.55.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5",
    "vue-tsc": "^1.0.7",
    "vuex": "^4.0.2"
  }
}
